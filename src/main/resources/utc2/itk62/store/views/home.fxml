                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Button>
                   <Button mnemonicParsing="false" onAction="#handleImportGoods" prefHeight="26.0" prefWidth="216.0" text="Import goods">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Button>
               </children>
               <HBox.margin>
