                     </VBox.margin>
                  </Button>
                   <Button mnemonicParsing="false" onAction="#handleHistory" prefHeight="26.0" prefWidth="234.0" text="History">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Button>
                   <Button mnemonicParsing="false" onAction="#handleBtnProduct" prefHeight="26.0" prefWidth="260.0" text="Product">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Button>
